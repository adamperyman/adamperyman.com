  "scripts": {
    "run:dev": "NODE_ENV=development PORT=3000 nodemon src/server --exec babel-node --presets @babel/env",
    "run:prod": "NODE_ENV=production nodemon build/server",
    "clean": "rimraf ./build",
    "clean:big": "yarn clean && rimraf ./node_modules && rm yarn.lock",
    "reset": "yarn clean:big && yarn && yarn build",
    "build": "yarn build:client && yarn build:server",
    "build:client": "NODE_ENV=production webpack -p --display-error-details --config=config/webpack.config.prod.js",
    "build:server": "babel src/server -s -d build/server --presets @babel/env",
    "lint": "node_modules/.bin/eslint --ignore-path .gitignore --ignore-pattern node_modules -- .",
    "test": "echo 'Hello, Test World!'"
